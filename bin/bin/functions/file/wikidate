#!/usr/bin/dash
# sdothum - 2016 (c) wtfpl

# File
# ══════════════════════════════════════════════════════════════════════════════

# ......................................................................... Date

# reset wiki journal date
if echo $@ | grep -q '[[:digit:]]*-[[:digit:]]*-[[:digit:]]*.wiki$' ;then
  touch -d$(echo $@ | sed -r 's/.*([0-9][0-9][0-9][0-9]-[0-9][0-9]-[0-9][0-9]).wiki/\1/') "$@"
fi

# vim: set ft=sh: #
