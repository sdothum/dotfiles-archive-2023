# ................................................................... rar binary

VSN=621

wget -P /tmp https://www.rarlab.com/rar/rarlinux-x64-${VSN}.tar.gz

tar xvf /tmp/rarlinux-x64-${VSN}.tar.gz
cd /tmp/rar
doas make install


