# ....................................................................... adsuck

original /etc/resolv.conf
sudo cp -v $HOME/build/cfg/etc/resolv.conf.adsuck /etc/resolv.conf
sudo cp -v $HOME/build/cfg/var/adsuck/* /var/adsuck/
service enable adsuck

